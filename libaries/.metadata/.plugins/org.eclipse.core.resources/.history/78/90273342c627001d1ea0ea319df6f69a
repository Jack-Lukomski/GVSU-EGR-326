void vGPIO_setPinNumber(s_GVSU_GPIO * s_userGPIO_ptr, e_GPIO_pinNumber e_userPinNumber)
{
    s_userGPIO_ptr->e_pinNumber = e_userPinNumber;
}

void vGPIO_setPort(s_GVSU_GPIO * s_userGPIO_ptr, e_GPIO_portNumber e_userPortNumber)
{
    s_userGPIO_ptr->e_portNumber = e_userPortNumber;
}

void vGPIO_setIO(s_GVSU_GPIO * s_userGPIO_ptr, e_GPIO_IO e_userIOType)
{
    s_userGPIO_ptr->e_IO = e_userIOType;
}

void vGPIO_InitlizePinGPIO(s_GVSU_GPIO * s_userGPIO_ptr, e_GPIO_IO e_userIOType, e_GPIO_portNumber e_userPortNumber, e_GPIO_pinNumber e_userPinNumber)
{

}
