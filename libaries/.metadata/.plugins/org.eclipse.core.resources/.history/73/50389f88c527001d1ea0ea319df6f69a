#include "msp.h"
#include <stdint.h>
#include <stdio.h>

typedef enum e_GPIO_IO
{
    input,
    output,
}s_GPIO_IO;

typedef enum e_GPIO_portNumber
{
    port1,
    port2,
    port3,
    port4,
    port5,
    port6,
    port7,
    port8,
    port9,
    port10,
};

typedef enum e_GPIO_pinNumber
{
    pin1,
    pin2,
    pin3,
    pin4,
    pin5,
    pin6,
    pin7,
    pin8,
    pin9,
    pinA = 0x0400,
    pinB = 0x0800,
    pinC = 0x1000,
    pinD = 0x2000,
    pinE = 0x4000,
    pinF = 0x8000,
};

typedef struct s_GVSU_GPIO
{
    s_GPIO_IO e_IO;
    e_GPIO_portNumber e_portNumber;
    e_GPIO_pinNumber e_pinNumber;
}s_GVSU_GPIO;

void vGPIO_setPinNumber(s_GVSU_GPIO * s_userGPIO_ptr, e_GPIO_pinNumber e_userPinNumber);
void vGPIO_setPort(s_GVSU_GPIO * s_userGPIO_ptr, e_GPIO_portNumber e_userPortNumber);
